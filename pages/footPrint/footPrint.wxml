<view>
  <view class="box" style="background: url('{{urlHost}}img/zuji.jpg');background-size: 100% 100%">
    <image bindtap="imageClick" class="xing2" src="{{imgFlag?(urlHost+'img/xing.png'):(urlHost+'img/xing2.png')}}" style="animation-name: {{xing2}};"></image>
  </view>
  <view class="jia1" wx:if="{{imgFlag1}}" style="animation-name: {{jia1}}">+1</view>
  <view class="head_css" style="bottom: {{heightValue/1.5+10}}px;color:{{titleColoe}}">{{titleText}}</view>
  <view class="title_css" style="bottom: {{heightValue/1.5+10}}px">谁来的勤:</view>
  <scroll-view scroll-y="true" class="box_paihang" style="height: {{heightValue/1.5}}px">
    <view class="list_css" wx:for="{{listData}}" wx:key="index">
      <view>
        <view style="float: left;line-height: 10vw;font-size: 7vw;color: #999;">{{index+1}}.</view>
        <image src="{{item.pathImg}}" class="pai_css"></image>
        <view class="name_css">{{item.name}}</view>
      </view>
      <view class="num_css">来过{{item.num}}次</view>
    </view>
  </scroll-view>
</view>